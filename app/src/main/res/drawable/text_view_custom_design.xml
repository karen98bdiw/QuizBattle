<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
            <corners
                android:radius="14dp"
                />
            <gradient
                android:angle="45"
                android:centerX="35%"
                android:centerColor="#dada77"
                android:startColor="#ecedd2"
                android:endColor="#75a746"
                android:type="linear"
                />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"
                />


        </shape>

    </item>
</selector>